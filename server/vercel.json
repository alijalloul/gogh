{
  "version": 2,
  "builds": [{
      "src": "server/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run postinstall"
      }
  }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "server/src/main.ts",
          "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
          "headers": {
                "Access-Control-Allow-Origin": "*"
            }
      }
  ]
}
