{
    "version": 2,
    "builds": [{
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run postinstall",
                "outputDirectory": "dist"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
  }
  